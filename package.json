{
  "type": "module",
  "version": "4.0.0",
  "scripts": {
    "dev": "vite",
    "build:example": "vue-tsc && vite build --mode example",
    "serve:example": "http-server ./dist-example -o",
    "svgo": "svgo -f src/assets/icons",
