    "preinstall": "npx only-allow pnpm",
    "commit": "git cz",
    "release": "bumpp"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@vueuse/core": "^10.7.0",
    "@vueuse/integrations": "^10.7.0",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "eruda": "^3.0.1",
    "floating-vue": "2.0.0-beta.24",
    "hotkeys-js": "^3.13.2",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "mockjs": "^1.1.0",
    "overlayscrollbars": "^2.4.5",
    "overlayscrollbars-vue": "^0.5.6",
    "pinia": "^2.1.7",
    "qs": "^6.11.2",
    "vconsole": "^3.15.1",
    "vexip-ui": "^2.2.22",
    "vue": "^3.3.12",
    "vue-m-message": "^4.0.2",
    "vue-router": "^4.2.5"
